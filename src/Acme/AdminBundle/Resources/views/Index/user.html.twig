{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Users</h1>

    <table class="records_list table table-bordered">
        <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.id }}</td>
                <td>{{ entity.username }}</td>
                <td>{{ entity.email }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}







